{
  "name": "@fdograph/balls-frenzy",
  "homepage": "http://fdograph.github.io/balls-frenzy",
  "repository": {
    "type": "git",
    "url": "https://github.com/fdograph/balls-frenzy.git"
  },
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@testing-library/user-event": "^9.0.0",
    "@types/jest": "^27.5.1",
    "@types/lodash": "^4.14.182",
    "@types/node": "^16.11.36",
    "@types/react-dom": "^17.0.17",
    "@types/styled-components": "^5.1.25",
    "gh-pages": "^4.0.0",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "5.0.1",
    "styled-components": "^5.0.1",
    "typescript": "~4.7.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "predeploy": "yarn run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "prettier"
    ],
    "plugins": [
      "react",
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "printWidth": 80,
          "trailingComma": "es5",
          "semi": true,
          "jsxSingleQuote": false,
          "singleQuote": true,
          "useTabs": false,
          "tabWidth": 2
        }
      ],
      "camelcase": [
        "error",
        {
          "properties": "always",
          "ignoreDestructuring": true,
          "ignoreImports": true
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react": "^16.9.20",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "prettier": "^1.19.1",
    "prettier-eslint": "^9.0.1"
  }
}
